getgenv().CRASH = true;
getgenv().BeAnnoying = true;
getgenv().SWORD = true;

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Test/My game--by jude/rev", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Random",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "Sword shit"
})

Tab:AddToggle({
	Name = "Get alot of swords",
	Default = (bool),
	Callback = function(bool)
		getgenv().SWORD = bool
		print(bool)
		if bool then
	SWORD()
		end
	end    
    
})





Tab:AddButton({
	Name = "Drop swords does a ton of damage",
	Callback = function()
      		for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
      if v.ClassName == "Tool" then
      v.CanBeDropped = true
      v.Parent =game.Players.LocalPlayer.Character
      wait()
      v.Parent = workspace.Terrain
end
end
  	end    
})



local Section = Tab:AddSection({
	Name = "other"
})

Tab:AddToggle({
	Name = "Spam drums ANNOYing af",
	Default = (bool),
	Callback = function(bool)
		getgenv().BeAnnoying = bool
		print(bool)
		if bool then
	BeAnnoying()
		end
	end    
    
})

Tab:AddButton({
	Name = "Kill all",
	Callback = function()
      		
local clickdetector =game:GetService("Workspace")["Smooth Block Model"].ClickDetector
fireclickdetector(clickdetector)

  	end    
})


local Tab = Window:MakeTab({
	Name = "Crashing",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


Tab:AddToggle({
	Name = "Crash server",
	Default = (bool),
	Callback = function(bool)
		getgenv().CRASH = bool
		print(bool)
		if bool then
	CRASH()
		end
	end    
    
})





function CRASH()
while getgenv().CRASH == true do
local clickdetector =game:GetService("Workspace").Crash1.Selection1.ClickDetector

fireclickdetector(clickdetector)
wait()
