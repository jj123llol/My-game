function SWORD()
    while  getgenv().SWORD == true do
        for _,v in pairs(game:GetService("Workspace").ToolGiver.Giver:GetDescendants()) do
if v:IsA("TouchTransmitter") then
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0) --0 is touch
wait()
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1) -- 1 is untouch

for _,v in pairs(game:GetService("Workspace").ToolGiver2.Giver:GetDescendants()) do
if v:IsA("TouchTransmitter") then
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0) --0 is touch
wait()
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1) -- 1 is untouch

end
end
end
end
end
end




function BeAnnoying()
    while getgenv().BeAnnoying == true do
        wait()
local clickdetector = game:GetService("Workspace")["Custom heavy drum1"].Crash.ClickDetector -- path to the click detector

fireclickdetector(clickdetector)


local clickdetector = game:GetService("Workspace")["Custom heavy drum1"].Hat.Model["crash cymbal"].ClickDetector -- path to the click detector

fireclickdetector(clickdetector)


local clickdetector = game:GetService("Workspace")["Custom heavy drum1"].Ride.Model["crash cymbal"].ClickDetector -- path to the click detector

fireclickdetector(clickdetector)


local clickdetector = game:GetService("Workspace")["Custom heavy drum2"].Crash.ClickDetector -- path to the click detector

fireclickdetector(clickdetector)


local clickdetector = game:GetService("Workspace")["Custom heavy drum2"].Hat.Model["crash cymbal"].ClickDetector -- path to the click detector

fireclickdetector(clickdetector)


local clickdetector = game:GetService("Workspace")["Custom heavy drum2"].Ride.Model["crash cymbal"].ClickDetector -- path to the click detector

fireclickdetector(clickdetector)

local clickdetector = game:GetService("Workspace")["Custom heavy drum3"].Crash.ClickDetector -- path to the click detector

fireclickdetector(clickdetector)


local clickdetector = game:GetService("Workspace")["Custom heavy drum3"].Hat.Model["crash cymbal"].ClickDetector -- path to the click detector

fireclickdetector(clickdetector)


local clickdetector = game:GetService("Workspace")["Custom heavy drum3"].Ride.Model["crash cymbal"].ClickDetector -- path to the click detector

